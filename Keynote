  <div class="navigation" style="position: fixed; bottom: 30px; right: 30px; display: flex; gap: 15px;">
    <button class="nav-button" onclick="previousSlide()">Previous</button>
    <button class="nav-button" onclick="nextSlide()">Next</button>
  </div>

  <script>
    let currentSlide = 0;
    const slides = document.querySelectorAll('.slide');

    function showSlide(index) {
      slides.forEach(slide => slide.classList.remove('active'));
      slides[index].classList.add('active');
    }

    function nextSlide() {
      currentSlide = (currentSlide + 1) % slides.length;
      showSlide(currentSlide);
    }

    function previousSlide() {
      currentSlide = (currentSlide - 1 + slides.length) % slides.length;
      showSlide(currentSlide);
    }

    document.addEventListener('keydown', function(event) {
      if (event.key === 'ArrowRight') nextSlide();
      if (event.key === 'ArrowLeft') previousSlide();
    });

    // Enhanced Chart Configurations
    const appleChartConfig = {
      responsive: true,
      maintainAspectRatio: false,
      plugins: {
        legend: {
          position: 'bottom',
          labels: {
            padding: 25,
            font: {
              family: '-apple-system',
              size: 16,
              weight: 500
            },
            color: '#86868b'
          }
        }
      },
      scales: {
        y: {
          beginAtZero: true,
          grid: {
            color: '#e5e5e5'
          },
          ticks: {
            font: {
              family: '-apple-system',
              size: 14
            },
            color: '#86868b',
            padding: 15
          }
        },
        x: {
          grid: {
            display: false
          },
          ticks: {
            font: {
              family: '-apple-system',
              size: 14
            },
            color: '#86868b',
            padding: 10
          }
        }
      }
    };

    // Initialize all charts when the document is loaded
    document.addEventListener('DOMContentLoaded', function() {
      // Market Share Growth
      new Chart(document.getElementById('marketShareChart').getContext('2d'), {
        type: 'line',
        data: {
          labels: ['2020', '2021', '2022', '2023', '2024'],
          datasets: [{
            label: 'Enterprise Market Share (%)',
            data: [28, 35, 45, 58, 82],
            borderColor: '#000000',
            backgroundColor: 'rgba(0, 0, 0, 0.1)',
            tension: 0.4,
            fill: true
          }]
        },
        options: appleChartConfig
      });

      // Feature Adoption
      new Chart(document.getElementById('featureAdoptionChart').getContext('2d'), {
        type: 'bar',
        data: {
          labels: ['Unified Dashboard', 'Advanced Security', 'Global Payments'],
          datasets: [{
            label: 'Adoption Rate (%)',
            data: [85, 92, 78],
            backgroundColor: '#000000',
            borderRadius: 8
          }]
        },
        options: appleChartConfig
      });

      // Implementation Timeline
      new Chart(document.getElementById('implementationChart').getContext('2d'), {
        type: 'line',
        data: {
          labels: ['Q1 2025', 'Q2 2025', 'Q3 2025', 'Q4 2025'],
          datasets: [{
            label: 'Deployment Progress (%)',
            data: [30, 60, 85, 100],
            borderColor: '#000000',
            backgroundColor: 'rgba(0, 0, 0, 0.1)',
            tension: 0.4,
            fill: true
          }]
        },
        options: appleChartConfig
      });

      // Financial Projections
      new Chart(document.getElementById('projectionChart').getContext('2d'), {
        type: 'bar',
        data: {
          labels: ['2025', '2026', '2027', '2028', '2029'],
          datasets: [{
            label: 'Projected Revenue ($B)',
            data: [8, 12, 16, 22, 28],
            backgroundColor: '#000000',
            borderRadius: 8
          }]
        },
        options: appleChartConfig
      });
    });
  </script>
